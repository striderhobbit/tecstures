{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.4.0",
    "generators": {
      "ng": {
        "inputSpec": "node_modules/contecst/build/tsoa/swagger.json",
        "generatorName": "typescript-angular",
        "output": "./src/core/openapi",
        "skipValidateSpec": true,
        "additionalProperties": {
          "basePath": "https://us-central1-contecst.cloudfunctions.net/api"
        },
        "modelNamePrefix": "FOO", // FIXME this is only a reminder
        "skipOverwrite": true // FIXME this is only a reminder
      }
    }
  }
}
